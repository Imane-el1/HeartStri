package cards;

import Heros.Hero;

public class SpellCard extends Card {
	
	private Card card;
	private String effect;
	private Hero player;
	
	public SpellCard() {
		
		card = new Card();
		player = new Hero();
		
		player.castOn(card);
	}
	
	public String getEffect() {
		return effect;
	}

	public void setEffect(String effect) {
		this.effect = effect;
	}


}

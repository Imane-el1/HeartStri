package cards;

import Heros.Hero;

public class SpellCard extends Card {
	
	private Card card;
	private String effect;
	private Hero player;
	
	card = new Card();
	player = new Hero();
	
	public String getEffect() {
		return effect;
	}

	public void setEffect(String effect) {
		this.effect = effect;
	}
	
	
	
	Hero.
	
	

}
